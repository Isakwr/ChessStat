{
    "manifest_version": 3,
    "name": "ChessStat",
    "description": "Get statistics from your chess games",
    "version": "1.0",
    "action":{
        "default_popup": "popup.html",
        "default_icon": "good_png.png"
    },
    "backgruond": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permission":[
        "storage",
        "webRequest",
        "background",
        "tabs",
        "webRequestBlocking"
    ],
    "host_permissions":[
        "*"
    ]

    
}